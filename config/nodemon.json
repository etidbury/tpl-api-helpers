{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {},
    "events": {
        "restart": ""
    },
    "watch": [
        "./src/routes",
        "./src/lib",
        "./src/services",
        "./src/util",
        "./src/config"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json"
}